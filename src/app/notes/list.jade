md-card(ng-repeat="note in vm.notes")
    md-card-content(ng-bind-html="note.getHtml()")
    div(class="md-actions" layout="row" layout-align="end center")
            md-button {{ note.user.first_name }} {{ note.user.last_name }}
            md-button(ui-sref="detail({id: note.id})")  {{ note.edited | date : 'yyyy-MM-dd HH:mm' }}

